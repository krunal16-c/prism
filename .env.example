# PRISM Environment Variables
# Copy this file to .env and fill in your values

# Google Gemini API Key (for NL Query in backend)
GOOGLE_API_KEY=your_google_api_key_here

# Anthropic API Key (optional, for Claude integration)
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# OpenAI API Key (for AI Agent in gov_agents)
OPENAI_API_KEY=your_openai_api_key_here

# Database URL (default SQLite)
DATABASE_URL=sqlite:///./prism.db

# MCP Server URLs (optional, for MCP integration)
MCP_TRANSPORTATION_URL=http://localhost:8001/sse
MCP_GOV_URL=http://localhost:8002/sse

# CORS Origins (comma-separated)
CORS_ORIGINS=http://localhost:3000
